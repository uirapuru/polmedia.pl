{% extends '::base.html.twig' %}

{% block title %}{{ category.getTitle() }}{% endblock %}

{% block body %}
<div class="hr">
    <h1>{{ category.getTitle() }}</h1>
</div>

<ul>
{% for video in videos %}
    <li><a href="{{ path("video",{id : video.getId()}) }}">{{ video.getTitle() }}</a></li>
{% endfor %}
</ul>
{% endblock %}