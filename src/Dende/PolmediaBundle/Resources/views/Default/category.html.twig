{% extends '::base.html.twig' %}

{% block title %}{{ category.getTitle() }}{% endblock %}

{% block body %}
Videos for {{ category.getTitle() }}:

<ul>
{% for video in videos %}
    <li><a href="{{ path("video",{id : video.getId()}) }}">{{ video.getTitle() }}</a></li>
{% endfor %}
</ul>
{% endblock %}